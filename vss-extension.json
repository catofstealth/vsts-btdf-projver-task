{
  "manifestVersion": 1,
  "public": true,
  "id": "BTDFProjectFileUpdaterTypeScript",
  "version": "0.1.2.1",
  "name": "BTDF Project File Updater (TypeScript)",
  "description": "Modifies the Product Version, Project Version and Product ID in the BTDFProj file prior to creating the MSI.",
  "publisher": "Catcomp",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "scopes": [
    "vso.build",
    "vso.build_execute"
  ],
  "categories": ["Azure Pipelines", "Azure Artifacts"],
  "tags": [
    "BTDF",
    "BizTalk",
    "Version",
    "GitVersion"
  ],
  "content": {
    "details": { "path": "overview.md" },
    "license": { "path": "LICENSE" }
  },
  "links": {
    "repository": {"uri": "https://github.com/catofstealth/vsts-btdf-projver-task"  },
    "support": { "uri": "https://github.com/catofstealth/vsts-btdf-projver-task/issues" }
  },
  "repository": { 
    "type": "git", 
    "uri": "https://github.com/catofstealth/vsts-btdf-projver-task" 
  }, 
  "contributions": [
    {
      "id": "vsts-btdf-projver-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildtask"
      }
    }
  ],
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "buildtask"
    }
  ]
}
